@model IEnumerable<Chasoliveira.Core.Dto.SkillDTO>
<div class="row">
    <div class="col-sm-1 col-md-2 mobmid">
        <span class="secicon fa fa-magic"></span>
    </div><!--icon end-->
    <div class="col-sm-11 col-md-10">
        <h3 class="mobmid">Technical skills </h3>
        @foreach (var item in Model)
        {
            <p>@item.Description</p>
            <div class="progress">
                <div class="progress-bar progress-bar-@item.Class" role="progressbar" aria-valuenow="@Convert.ToInt32(item.Progress)" aria-valuemin="0" aria-valuemax="100" style="width: @Convert.ToInt32(item.Progress)%">
                    <span class="sr-only">@Convert.ToInt32(item.Progress)% Complete</span>
                </div>
            </div>
        }
    </div>
</div>